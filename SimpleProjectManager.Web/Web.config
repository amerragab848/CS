          <add path="DXX.axd" verb="*" type="DevExpress.ExpressApp.Web.XafHttpHandler, DevExpress.ExpressApp.Web.v19.1, Version=19.1.3.0, culture=neutral, PublicKeyToken=b88d1754d700e49a" />    
